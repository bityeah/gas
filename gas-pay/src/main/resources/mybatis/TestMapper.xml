<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.demo.gas.pay.dao.mapper.TestMapper">
    <select id="getAccount" resultType="cn.demo.gas.pay.model.Account">
        select * from `order` where id = ${id}
    </select>

    <insert id="insertOrder" parameterType="cn.demo.gas.pay.model.Order">
        insert into `order` (id, uid, `name`) values (${id}, ${uid}, '${name}')
    </insert>

    <select id="queryOrderList" resultType="cn.demo.gas.pay.model.Order">
        select * from `order` where (id BETWEEN 1 and 1000) and (uid BETWEEN 1 and 1000) and `name` like 'chushou1%' order by id desc limit 1, 100
    </select>

</mapper>